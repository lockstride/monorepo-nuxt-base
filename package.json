{
  "name": "monorepo-nuxt-base",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "//-- GLOBAL SCRIPTS --//": "--------------------------------",
    "start": "nx run-many --target=serve --projects=api,webapp,marketing",
    "build": "nx run-many --target=build --projects=api,webapp,marketing,ui,utils,prisma,env-run",
    "format:check": "nx format:check --all",
    "format:write": "nx format:write --all --sort-root-tsconfig-paths=false",
    "lint:check": "nx run-many --target=lint --projects=api,webapp,marketing,ui,utils,prisma,env-run,gh-actions",
    "lint:fix": "nx run-many --target=lint --projects=api,webapp,marketing,ui,utils,prisma,env-run,gh-actions --fix",
    "test-unit": "nx run-many --target=test-unit --projects=api,webapp,marketing,env-run",
    "test-e2e": "nx run-many --target=test-e2e --projects=api,webapp,marketing",
    "test-coverage": "nx run-many --target=test-coverage --projects=api,webapp,marketing,env-run",
    "typespec:validate": "pnpm exec tsp compile specs --no-emit",
    "typespec:codegen": "pnpm exec tsp compile specs --emit @typespec/openapi3",
    "all-check": "pnpm format:check && pnpm lint:check && pnpm typespec:validate && pnpm build && pnpm test-coverage && pnpm test-e2e",
    "//-- COMPONENT SCRIPTS --//": "--------------------------------",
    "api:start": "nx run api:serve",
    "api:build": "nx build api",
    "api:test-unit": "nx run api:test-unit",
    "api:test-e2e": "nx run api:test-e2e",
    "api:lint": "nx lint api",
    "api:lint:fix": "nx lint api --fix",
    "marketing:start": "nx run marketing:serve",
    "marketing:build": "nx build marketing",
    "marketing:test-unit": "nx run marketing:test-unit",
    "marketing:test-e2e": "nx run marketing:test-e2e",
    "marketing:test-e2e:open": "nx run marketing:test-e2e --configuration=open",
    "marketing:lint": "nx lint marketing",
    "marketing:lint:fix": "nx lint marketing --fix",
    "webapp:start": "nx run webapp:serve",
    "webapp:build": "nx build webapp",
    "webapp:test-unit": "nx run webapp:test-unit",
    "webapp:test-e2e": "nx run webapp:test-e2e",
    "webapp:test-e2e:open": "nx run webapp:test-e2e --configuration=open",
    "webapp:lint": "nx lint webapp",
    "webapp:lint:fix": "nx lint webapp --fix",
    "//-- DATABASE SCRIPTS --//": "--------------------------------",
    "db:up": "nx run prisma:db:setup",
    "db:down": "nx run prisma:db:teardown",
    "db:migrate": "nx run prisma:db:migrate",
    "db:reset": "nx run prisma:db:migrate-reset",
    "db:seed": "nx run prisma:db:seed",
    "db:studio": "nx run prisma:db:studio",
    "db:test:studio": "nx run prisma:db:studio:e2e-docker",
    "//-- DOCKER SCRIPTS --//": "--------------------------------",
    "docker:up": "pnpm env-run --env=dev-docker -- docker compose -f infra/compose.local.yml up -d --build",
    "docker:down": "pnpm env-run --env=dev-docker -- docker compose -f infra/compose.local.yml down",
    "//-- Setup & Utilities Scripts --//": "--------------------------------",
    "pre-push": "pnpm format:check && pnpm lint:check && pnpm typespec:validate && pnpm build && pnpm audit --audit-level moderate && pnpm test-coverage",
    "pre-commit": "lint-staged",
    "dev-setup": "pnpm husky && cypress install && chmod +x .husky/pre-push && chmod +x .husky/pre-commit"
  },
  "keywords": [],
  "author": {
    "name": "Lockstride",
    "email": "hello@lockstride.ai",
    "url": "https://lockstride.ai"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.28.2",
  "type": "module",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@parcel/watcher",
      "@prisma/engines",
      "@swc/core",
      "cypress",
      "esbuild",
      "less",
      "nx",
      "prisma",
      "unrs-resolver",
      "vue-demi"
    ],
    "ignoredBuiltDependencies": [
      "@scarf/scarf"
    ],
    "overrides": {
      "lodash": ">=4.17.23",
      "hono": ">=4.11.7"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@nuxt/ui": "^4.3.0",
    "@nx/cypress": "22.4.5",
    "@nx/eslint": "22.4.5",
    "@nx/eslint-plugin": "22.4.5",
    "@nx/js": "22.4.5",
    "@nx/nest": "22.4.5",
    "@nx/node": "22.4.5",
    "@nx/nuxt": "22.4.5",
    "@nx/vite": "22.4.5",
    "@nx/vitest": "22.4.5",
    "@nx/web": "22.4.5",
    "@nx/webpack": "22.4.5",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "1.15.11",
    "@swc/helpers": "~0.5.17",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.3.2",
    "@typespec/compiler": "^1.9.0",
    "@typespec/http": "^1.9.0",
    "@typespec/openapi": "^1.9.0",
    "@typespec/openapi3": "^1.9.0",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "cypress": "15.10.0",
    "env-run": "workspace:*",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.1.1",
    "h3": "1.15.5",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jiti": "2.6.1",
    "jsdom": "~26.1.0",
    "lint-staged": "^16.1.6",
    "nuxt": "4.3.0",
    "nuxt-icon": "1.0.0-beta.7",
    "nx": "22.4.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^7.3.0",
    "rimraf": "^6.1.2",
    "run-script-webpack-plugin": "^0.2.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.20.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.43.0",
    "unplugin-swc": "^1.5.7",
    "vite": "^7.1.12",
    "vitest": "4.0.18",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@fastify/static": "^9.0.0",
    "@fastify/view": "^11.1.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:noop"
      },
      "test-unit": {
        "executor": "nx:noop"
      },
      "test-e2e": {
        "executor": "nx:noop"
      },
      "test-coverage": {
        "executor": "nx:noop"
      }
    }
  }
}
